doctype
html
    head
        meta(charset='utf-8')
        title jadeDemo
        style.
            div {width:100px;height:100px;background:red;text-align:center;line-height:100px}
            div.active {border:1px solid yellow}
    body
        -let a=0
        while a<12
            if a%4==0 && a!=0
                div.active=a++
            else
                div=a++     
